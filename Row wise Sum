#include <iostream>
#include <vector>
using namespace std;

void  rowSum ( int arr[3][4] , int n , int m ){
    
   cout << "Printing the sum ->" << endl;
   
    for(int i = 0; i< n; i++){
        int sum = 0;
        for(int j = 0; j< m ; j++ ){
            sum += arr[i][j];
            }
            cout << sum << " "; 
        }

    cout << endl;
    
    for(int i = 0; i< n; i++){
        int sum = 0;
        for(int j = 0; j< m ; j++ ){
            cout <<  arr[i][j];
            }
            cout << endl;
    
        }

    cout << endl;
}


int main() {
    int arr[3][4];
 
   for(int row = 0; row  < 3; row ++){
       for(int col = 0; col < 4; col++){
           cin >> arr[row][col];
       }
   }
   
    rowSum(arr , 3 , 4);
     
   
}



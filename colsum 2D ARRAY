#include <iostream>
#include <vector>
using namespace std;

void  colSum ( int arr[3][4] , int n , int m ){
    
   cout << "Printing the sum ->" << endl;
   
    for(int i = 0; i< m; i++){
        int sum = 0;
        for(int j = 0; j< n ; j++ ){
            
            sum += arr[j][i];
            }
            cout << sum << " "; 
        }

    cout << endl;
    

}


int main() {
    int arr[3][4];
 
   for(int row = 0; row  < 3; row ++){
       for(int col = 0; col < 4; col++){
           cin >> arr[row][col];
       }
   }
   
    colSum(arr , 3 , 4);
     
   
}


